{
	"name": "ayus theme",
	"type": "dark",
	"colors": {
		"editor.background": "#262626",
		"editor.foreground": "#8f8f8f",
		"activityBarBadge.background": "#00b670",
		"sideBarTitle.foreground": "#bbb",
		"editor.lineHighlightBackground": "#111",
		"editor.lineHighlightBorder": "#202020",
		"editor.selectionBackground": "#444",
		"sideBar.background": "#262626",
		"list.activeSelectionBackground": "#097160",
		"menu.selectionBackground": "#097160",
		"editorBracketMatch.border": "#07f7",
		"editorBracketMatch.background": "#07f7",
		"editorCodeLens.foreground": "#888",
		/**/
		"button.background": "#097160",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#0f9268",
		/**/
		"focusBorder": "#00c795",
		"inputOption.activeBackground": "#00c79566",
		"notebook.cellInsertionIndicator": "#00c795",
		"notebook.focusedEditorBorder": "#00c795",
		/**/
		"editorSuggestWidget.background": "#252526",
		"editorSuggestWidget.border": "#454545",
		"editorSuggestWidget.foreground": "#8f8f8f",
		"editorSuggestWidget.highlightForeground": "#00c795",
		"editorSuggestWidget.selectedBackground": "#064a33",
		/**/
		"list.dropBackground": "#064a33",
		"list.focusBackground": "#064a33",
		/**/
		"notificationLink.foreground": "#37ffde",
		"pickerGroup.foreground": "#37ffde",
		"textLink.activeForeground": "#37ffde",
		"textLink.foreground": "#37ffde",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#999999"
			}
		},
		{
			"name": "Object, Property",
			"scope": [
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#aaccaa"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type"
			],
			"settings": {
				"foreground": "#a26fff"
			}
		},
		{
			"name": "Storage, Modifier",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff3399"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword.type"
			],
			"settings": {
				"foreground": "#4aa6f1"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#fafafa"
			}
		},
		{
			"name": "Return",
			"scope": [
				"keyword.control.flow.return"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.function.member",
				"entity.name.variable.local.cs"
			],
			"settings": {
				"foreground": "#93e6ff"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#ff815b"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
				"punctuation.definition.string",
			],
			"settings": {
				"foreground": "#88ff59"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#fff06b"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
